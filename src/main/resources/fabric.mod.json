{
  "schemaVersion": 1,
  "id": "community-addon",
  "version": "${version}",
  "name": "Meteor Community Addon",
  "description": "A mod made by the Meteor Community. Think an anarchy server but as an addon.",
  "authors": [
    "everyone"
  ],
  "contact": {
    "repo": "https://github.com/RickyTheRacc/meteor-community-addon"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "communityaddon.Addon"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
